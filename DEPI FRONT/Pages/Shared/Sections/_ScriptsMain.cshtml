

<environment include="Development">
    @* <!-- Vendor Javascript (Require in all Page) --> *@
    <script src="~/js/vendor.js"></script>

    @* <!-- App Javascript (Require in all Page) --> *@
    <script src="~/js/app.js"></script>

    @* <!-- Vector Map Js --> *@
    <script src="~/vendor/jsvectormap/js/jsvectormap.min.js"></script>
    <script src="~/vendor/jsvectormap/maps/world-merc.js"></script>
    <script src="~/vendor/jsvectormap/maps/world.js"></script>

    @* <!-- Dashboard Js --> *@
    <script src="~/js/pages/dashboard.js"></script>
</environment>

<environment exclude="Development">
    @* <!-- Vendor Javascript (Require in all Page) --> *@
    <script src="~/js/vendor.js" asp-append-version="true"></script>
    @* <!-- App Javascript (Require in all Page) --> *@
    <script src="~/js/app.js" asp-append-version="true"></script>
    @* <!-- Vector Map Js --> *@
    <script src="~/vendor/jsvectormap/js/jsvectormap.min.js" asp-append-version="true"></script>
    <script src="~/vendor/jsvectormap/maps/world-merc.js" asp-append-version="true"></script>
    <script src="~/vendor/jsvectormap/maps/world.js" asp-append-version="true"></script>

    @* <!-- Dashboard Js --> *@
    <script src="~/js/pages/dashboard.js" asp-append-version="true"></script>
</environment>

